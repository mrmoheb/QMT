        <messages/>
        <name>SaveAnswers</name>
        <order/>
        <script><![CDATA[function onSubmit() {
	console.warn("Start the Script");
   var count = 0;
   var allAnswerFields = $j('[id^=answer]');
   var fullAnswerFieldText="";
	console.warn("JQUERY IS READ");
   while(allAnswerFields.length > count) 
   {
	   if(count==0)
	   {
		fullAnswerFieldText += allAnswerFields[count].value;
	   }
	   else
	   {
	    fullAnswerFieldText +=","+ allAnswerFields[count].value;
       }
   count ++;
   }
	console.warn(fullAnswerFieldText);
    g_form.setValue('answers',fullAnswerFieldText);
}]]></script>
